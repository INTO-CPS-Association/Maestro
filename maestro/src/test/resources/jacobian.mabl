


simulation
import  FMI2;
import InitializerUsingCOE
{

FMI2 fmuA = load("FMI2", "path/to/FMUA.fmu");
FMI2 fmuB = load("FMI2", "path/to/FMUB.fmu");

FMI2Component a = fmuA.instantiate("A", true);
FMI2Component b = fmuB.instantiate("B", true);

expand initialize(a,b);


expand jacobian(a,b);

expand cleanup(a,b);


}