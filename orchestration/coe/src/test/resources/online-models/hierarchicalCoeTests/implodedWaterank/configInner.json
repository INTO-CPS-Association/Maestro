{
	"fmus": {
		"{ext}": "external://external",
		"{tank}": "singlewatertank-20sim.fmu",
		"{crtl}": "watertankcontroller-c.fmu"
	},
	"connections": {
		"{tank}.tank.level": ["{crtl}.crtl.level"],
		"{ext}.abekat.{tank}.tank.valvecontrol": ["{tank}.tank.valvecontrol"]
	},
	"parameters": {
		"{crtl}.crtl.maxlevel": 1
	},
	"liveGraphColumns": 1,
	"liveGraphVisibleRowCount": 1,
	"logVariables": {
		"{tank}.tank": ["level"],
		"{crtl}.crtl": ["valve"]
	},
	"algorithm": {
		"type": "fixed-step",
		"size": 0.1
	},
	"parallelSimulation": false,
	"stabalizationEnabled": false,
	"global_absolute_tolerance": 0,
	"global_relative_tolerance": 0.01,
	"simulationProgramDelay": false,
	"livestream": {},
	"hasExternalSignals": true
}
