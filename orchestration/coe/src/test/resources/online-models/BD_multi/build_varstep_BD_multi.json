{
	"fmus":{
		"{sine}":"target/online-cache/sine.fmu",
		"{pump}":"target/online-cache/pump.fmu",
		"{sink}":"target/online-cache/sink.fmu"
	},
	"connections":{
		"{sine}.sine.y":["{sink}.sink.sig1"],
		"{pump}.pump1.mdot_out":["{sink}.sink.sig2"]
	},
	"parameters":{
		"{sine}.sine.amp":1,
		"{sine}.sine.sineFreq":0.5,
		"{sine}.sine.start":1,
		"{pump}.pump1.mdotInit":4,
		"{pump}.pump1.tBegin":3,
		"{pump}.pump1.tLength":1,
		"{pump}.pump1.h":-10
	},
	"algorithm":{
		"type":"var-step",
		"size":[
			1E-3,
			0.2
		],
		"initsize":1E-2,
		"constraints":{
		"bd1":{
				"type":"boundeddifference",
				"ports":[
					"{sine}.sine.y"
				],
				"abstol":0.55,
				"reltol":5.0
			},
		"bd2":{
				"type":"boundeddifference",
				"ports":[
					"{pump}.pump1.mdot_out"
				],
				"abstol":1.0,
				"reltol":5.0
			}							
		}
	}	
}